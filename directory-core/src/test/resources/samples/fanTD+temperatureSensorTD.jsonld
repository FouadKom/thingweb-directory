{
	"@context": [
		"https://www.w3.org/2019/wot/td/v1",
		{
			"geo": "http://www.w3.org/2003/01/geo/wgs84_pos#",
			"saref": "http://uri.etsi.org/m2m/saref#",
			"Sensor": "saref:Sensor",
			"Temperature": "saref:Temperature",
			"Toggle": "saref:ToggleCommand",
			"unit": "saref:isMeasuredIn",
			"celsius": "http://www.wurvoc.org/vocabularies/om-1.8/degree_Celsius"
		}
	],
	"@graph": [
		{
			"id": "urn:Fan",
			"@type": ["Thing", "Sensor"],
			"name": "Fan",
			"base": "coap:///www.example.com:5686/Fan",
			"properties": {
				"myThreshold": {
					"@type": "Temperature",
					"type": "number",
					"unit": "celsius",
					"writable": true,
					"geo:location": "room_4",
					"forms": [{
						"href": "threshold",
						"mediaType": "application/json"
					}]
				}
			},
			"actions": {
				"fanOnOff": {
					"@type": "Toggle",
					"name": "fanOnOff",
					"input": {
						"@type": "OnOff",
						"type": "boolean"
					},
					"forms": [{
						"href": "toggle",
						"mediaType": "application/json"
					}]
				},
				"fanOnOffTempAction": {
					"@type": "Toggle",
					"name": "fanOnOffTempAction",
					"input": {
						"@type": "Temperature",
						"type": "number",
						"unit": "celsius"
					},
					"geo:location": "room_4",
					"forms": [{
						"href": "fanOnOffTempAction",
						"mediaType": "application/json"
					}]
				}
			}
		},
		{	
			"id": "urn:Temp",
			"@type": ["Thing", "Sensor"],
			"name": "myTempSensor",
			"base": "coap:///www.example.com:5687/temp",
			"properties": {
				"myTemp": {
					"@type": "Temperature",
					"unit": "celsius",
					"reference": "threshold",
					"name": "myTemp",
					"type": "number",
					"writable": false,
					"forms": [{
						"href": "val",
						"mediaType": "application/json"
					}]
				},
				"myThreshold": {
					"@type": "Temperature",
					"unit": "celsius",
					"name": "myThreshold",
					"type": "number",
					"writable": true,
					"forms": [{
						"href": "threshold",
						"mediaType": "application/json"
					}]
				}
			},
			"events": {
				"myChange": {
					"type": "number",
					"name": "myChange",
					"property": "temp",
					"forms": [{
						"href": "val/changed",
						"mediaType": "application/json"
					}]
				},
				"myWarning": {
					"type": "number",
					"name": "myWarning",
					"forms": [{
						"href": "val/high",
						"mediaType": "application/json"
					}]
				}
			}
		}
	]
}